<%- include('partials/header') -%>

        <% if (typeof success !== 'undefined') { %>
            <div class="toast alert-dismissible fade">
            <% if (success) { %>
                <div class="alert alert-success">
                Your message has been sent successfully!
                </div>
            <% } else { %>
                <div class="alert alert-danger">
                There was an error sending your message. Please try again.
                </div>
            <% } %>
            <button type="button" class="close" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
            </div>
        <% } %>

        <!-- Page Title -->
        <section class="page-title alternat-2 centred" style="background-image: url(assets/images/background/page-title.jpg);">
            <div class="auto-container">
                <div class="content-box">
                    <div class="title">
                        <h1>Contact Us</h1>
                    </div>
                    <ul class="bread-crumb clearfix">
                        <li><a href="/">Home</a></li>
                        <li>Contact</li>
                    </ul>
                </div>
            </div>
        </section>
        <!-- End Page Title -->


        <!-- google-map-section -->
        <section class="google-map-section">
            <div class="map-inner">
                <div class="google-map" id="contact-google-map" data-map-lat="40.712776" data-map-lng="-74.005974" data-icon-path="assets/images/icons/map-marker.png" data-map-title="Brooklyn, New York, United Kingdom" data-map-zoom="12" data-markers='{
                        "marker-1": [40.712776, -74.005974, "<h4>Branch Office</h4><p>77/99 New York</p>","assets/images/icons/map-marker.png"]
                    }'>

                </div>
            </div>
        </section>
        <!-- google-map-section end -->


        <!-- contact-section -->
        <section class="contact-section">
            <div class="auto-container">
                <div class="inner-content">
                    <div class="title-inner centred">
                        <div class="text">
                            <h2>Any Questions? Drop Us A Line</h2>
                            <p>Want to reach us? Have a suggestion? To us, our customers are family.<br /> We're always happy to hear from you. Let us know any of your thoughts<br /> and we will get back to you as soon as possible.</p>
                        </div>
                        <!-- <div class="link">
                            <a href="/contact"><i class="fal fa-map-marker-alt"></i>find a service center near you</a>
                        </div> -->
                    </div>
                    <form method="post" action="/contact" id="contact-form" class="default-form"> 
                        <div class="row clearfix">
                            <div class="col-lg-4 col-md-6 col-sm-12 form-group">
                                <input type="text" name="username" placeholder="Full Name *" required="">
                            </div>
                            <div class="col-lg-4 col-md-6 col-sm-12 form-group">
                                <input type="email" name="email" placeholder="Email *" required="">
                            </div>
                            <div class="col-lg-4 col-md-12 col-sm-12 form-group">
                                <input type="text" name="phone" placeholder="Phone *" required="">
                            </div>
                            <div class="col-lg-12 col-md-12 col-sm-12 form-group">
                                <textarea name="message" placeholder="Your Message ..."></textarea>
                            </div>
                            <div class="col-lg-12 col-md-12 col-sm-12 form-group message-btn centred">
                                <button class="theme-btn-one" type="submit" ><span class="btn-shape"></span>send message</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </section>
        <!-- contact-section end -->


        <!-- contact-info-two -->
        <section class="contact-info-two centred">
            <div class="auto-container">
                <div class="row clearfix">
                    <div class="col-lg-4 col-md-6 col-sm-12 info-column">
                        <div class="single-info-box">
                            <div class="icon-box">
                                <div class="icon"><i class="far fa-map-marker-alt"></i></div>
                            </div>
                            <h3>Office Location</h3>
                            <p>All around DFW Area  -  USA</p>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-6 col-sm-12 info-column">
                        <div class="single-info-box">
                            <div class="icon-box">
                                <div class="icon"><i class="far fa-phone"></i></div>
                            </div>
                            <h3>Phone Number</h3>
                            Mob: <a href="tel:+19723027710">+1 972 302 7710</a></p>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-6 col-sm-12 info-column">
                        <div class="single-info-box">
                            <div class="icon-box">
                                <div class="icon"><i class="far fa-envelope"></i></div>
                            </div>
                            <h3>Send Message</h3>
                            <p><a href="mailto:info@westsidegates.com">info@westsidegates.com</a></p>
                        </div>
                    </div>
                </div>
                <div class="other-text"><h3><span>Our Showroom Hours Are:</span> All Week : 7:00 AM - 8:00 PM </h3></div>
            </div>
        </section>
        <!-- contact-info-two end -->


        <!-- clients-section -->
        <section class="clients-section contact-page">
            <div class="auto-container">
                <div class="four-item-carousel owl-carousel owl-theme owl-dots-none owl-nav-none">
                    <figure class="clients-logo"><img src="assets/images/clients/clients-logo-1.png" alt=""></figure>
                    <figure class="clients-logo"><img src="assets/images/clients/clients-logo-2.png" alt=""></figure>
                    <figure class="clients-logo"><img src="assets/images/clients/clients-logo-3.png" alt=""></figure>
                    <figure class="clients-logo"><img src="assets/images/clients/clients-logo-4.png" alt=""></figure>
                </div>
            </div>
        </section>
        <!-- clients-section end -->


<%- include('partials/footer') -%>